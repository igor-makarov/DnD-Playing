---
import WeaponAttackTableReact from "./WeaponAttackTable/WeaponAttackTable.tsx";
import type { Character } from "@/js/character/Character.ts";

interface Props {
  character: Character;
}

const { character } = Astro.props;

const weaponAttacks = character.getWeaponAttacks();
const damageAddons = character.getWeaponAttackAddons();
---

<WeaponAttackTableReact client:load weaponAttacks={weaponAttacks} damageAddons={damageAddons} />
