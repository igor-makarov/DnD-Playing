---
import CheckCell from './CheckCell.tsx';
const { skill, proficiency, bonus, advantage } = Astro.props;
---
<tr>
  <td><span class="mono">[{proficiency.symbol}]</span> {skill.skill} ({skill.modifier})</td>
  <td class="checkCell modifier">
    <CheckCell client:only="react" bonus={bonus} advantage={advantage} />
  </td>
</tr>
