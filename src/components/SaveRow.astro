---
import CheckCell from "./CheckCell.tsx";
const { ability, proficiency, check, advantage } = Astro.props;
---

<tr>
  <td><span class="mono">[{proficiency.symbol}]</span> {ability}</td>
  <td class="checkCell">
    <CheckCell client:only="react" check={check} advantage={advantage} />
  </td>
</tr>
