    "@types/mdx": "^2.0.13",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "isbot": "^5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.10.1",
    "vite": "^7.2.7"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/preset-react": "^7.28.5",
