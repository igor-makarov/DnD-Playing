    "@types/xml2js": "^0.4.14",
    "@vitest/coverage-v8": "^4.0.12",
    "@vitest/ui": "^4.0.12",
    "dompurify": "^3.3.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "serve": "^14.2.5",
    "tsx": "^4.21.0",
